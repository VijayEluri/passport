<decorators defaultdir="/WEB-INF/view/decorators">
    <excludes>
        <pattern>/40*.jsp</pattern>
        <pattern>/*ajax=true*</pattern>
        <pattern>/scripts/dojo/*</pattern>
        <pattern>/struts/dojo/*</pattern> 
        <pattern>/resources/*</pattern>
        <pattern>/statics/scripts/dojo/*</pattern>
        
        <pattern>/app/serviceValidate*</pattern>
        <pattern>/app/samlValidate*</pattern>
    </excludes> 
    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
</decorators>
